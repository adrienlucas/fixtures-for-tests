{
    "name": "adrien/fixtures-for-tests",
    "description": "A set of traits to be used in test cases to load fixtures.",
    "license": "MIT",
    "keywords": ["generator", "code generator", "tdd", "test-driven-development"],
    "authors": [
        {
            "name": "Adrien LUCAS",
            "email": "contact@adrienlucas.net"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "doctrine/data-fixtures": "^1.3"
    },
    "autoload": {"psr-4": {"Adrien\\FixturesForTests\\": "src"}},
    "autoload-dev": {"psr-4": {"Adrien\\FixturesForTests\\Tests\\": "tests"}},
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "doctrine/orm": "^2.9",
        "doctrine/phpcr-odm": "^1.4",
        "jackalope/jackalope": "^1.3",
        "jackalope/jackalope-doctrine-dbal": "^1.3",
        "doctrine/mongodb-odm": "^2.2.0",
        "symfony/framework-bundle": "^5.3"
    },
    "config": {"platform": {"ext-mongo": "1.6.7"}}
}
